{
  "name": "oanhnn/laravel-handlers",
  "type": "library",
  "description": "Using handler class instead of controller class in Laravel 5.5+ application",
  "keywords": [
    "laravel",
    "package",
    "handler",
    "actions",
    "controller"
  ],
  "homepage": "https://github.com/oanhnn/laravel-handlers",
  "license": "MIT",
  "authors": [
    {
      "name": "Oanh Nguyen",
      "email": "oanhnn.bk@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "^5.5"
  },
  "require-dev": {
    "mockery/mockery": "^0.9",
    "orchestra/testbench": "^3.5",
    "phpunit/phpunit": "~6.1 || ~7.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\Handlers\\HandlersServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Handlers\\": "src/"
    }
  },
  "scripts": {
    "phpcbf": "php vendor/squizlabs/php_codesniffer/bin/phpcbf",
    "phpcs": "php vendor/squizlabs/php_codesniffer/bin/phpcs",
    "phpunit": "php vendor/phpunit/phpunit/phpunit --coverage-html storage/coverage"
  },
  "support": {
    "issues": "https://github.com/oanhnn/laravel-handlers/issues",
    "source": "https://github.com/oanhnn/laravel-handlers"
  }
}
